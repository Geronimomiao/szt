.container .msg {
  background-color: #f0f0f0;
  height: 65vh;
  padding-top: 2vh;
}

.container .msg .content {
  margin-left: 5%;
  width: 90%;
  background-color: #d3ccc7;
  height: 55vh;
  border-radius: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .msg .content .block {
  border-radius: 4%;
}

.container .msg .content .cu-form-group {
  background-color: rgba(0, 0, 0, 0.05);
  border-top: 0px;
}

.container .msg .content .border-bm {
  border-bottom: 1px solid #f0f0f0;
}



.content-back {
  height: 55vh;
}

.canvas {
  width: 100%;
  height: 300px;
}



